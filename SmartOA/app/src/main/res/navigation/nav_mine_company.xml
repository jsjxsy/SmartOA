<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_mine_company"
    app:startDestination="@id/mineCompanyFragment">
    <fragment
        android:id="@+id/mineCompanyFragment"
        android:name="com.gx.smart.smartoa.activity.ui.company.MineCompanyFragment"
        android:label="fragment_mine_company"
        tools:layout="@layout/fragment_mine_company">
        <action
            android:id="@+id/action_mineCompanyFragment_to_mineCompanySelectAreaFragment"
            app:destination="@id/mineCompanySelectAreaFragment" />
    </fragment>
    <fragment
        android:id="@+id/mineCompanySelectAreaFragment"
        android:name="com.gx.smart.smartoa.activity.ui.company.MineCompanySelectAreaFragment"
        android:label="fragment_mine_company_select_area"
        tools:layout="@layout/fragment_mine_company_select_area">
        <action
            android:id="@+id/action_mineCompanySelectAreaFragment_to_mineCompanySelectCompanyFragment"
            app:destination="@id/mineCompanySelectCompanyFragment" />
    </fragment>
    <fragment
        android:id="@+id/mineCompanySelectCompanyFragment"
        android:name="com.gx.smart.smartoa.activity.ui.company.MineCompanySelectCompanyFragment"
        android:label="fragment_mine_company_select_company"
        tools:layout="@layout/fragment_mine_company_select_company">
        <action
            android:id="@+id/action_mineCompanySelectCompanyFragment_to_mineCompanyEmployeesFragment"
            app:destination="@id/mineCompanyEmployeesFragment" />
    </fragment>

    <fragment
        android:id="@+id/mineCompanyEmployeesFragment"
        android:name="com.gx.smart.smartoa.activity.ui.company.MineCompanyEmployeesFragment"
        android:label="fragment_mine_company_employees"
        tools:layout="@layout/fragment_mine_company_employees" />

</navigation>