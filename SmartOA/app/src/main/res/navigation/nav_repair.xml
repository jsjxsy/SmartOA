<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_login"
    app:startDestination="@id/repairFragment">
    <fragment
        android:id="@+id/repairFragment"
        android:name="com.gx.smart.smartoa.activity.ui.repair.RepairFragment"
        android:label="repair_fragment"
        tools:layout="@layout/repair_fragment" >
        <action
            android:id="@+id/action_repairFragment_to_repairTypeFragment"
            app:destination="@id/repairTypeFragment" />
        <action
            android:id="@+id/action_repairFragment_to_repairRecordFragment"
            app:destination="@id/repairRecordFragment" />
    </fragment>
    <fragment
        android:id="@+id/repairTypeFragment"
        android:name="com.gx.smart.smartoa.activity.ui.repair.RepairTypeFragment"
        android:label="repair_type_fragment"
        tools:layout="@layout/repair_type_fragment" />
    <fragment
        android:id="@+id/repairRecordFragment"
        android:name="com.gx.smart.smartoa.activity.ui.repair.RepairRecordFragment"
        android:label="fragment_repair_record_list"
        tools:layout="@layout/fragment_repair_record_list" />
</navigation>